{
  "name": "youtube-recommendation-scraper-api",
  "version": "0.3.0",
  "keywords": [
    "Youtube",
    "Recommendation"
  ],
  "description": "Youtube Recommendation Scraper",
  "author": "Luciano Frizzera <lucaju@gmail.com> (luciano.fluxo.art.br)",
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsup db/index.ts jobs/index.ts src/index.ts --format esm,cjs --dts",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "npm run build -- --watch --onSuccess \"node --watch dist/src/index.js\"",
    "start": "node dist/src/index.js",
    "gendoc": "tsup swagger.ts --format esm,cjs --dts --onSuccess \"node --watch dist/swagger.js\""
  },
  "dependencies": {
    "agenda": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "celebrate": "^15.0.3",
    "dotenv": "^16.4.4",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "fs-extra": "^11.2.0",
    "helmet": "^7.1.0",
    "joi": "^17.12.1",
    "joi-tz": "^4.1.1",
    "jsonwebtoken": "^9.0.2",
    "kleur": "^4.1.5",
    "loglevel": "^1.9.1",
    "luxon": "^3.4.4",
    "mongoose": "^8.1.2",
    "node-emoji": "^2.1.3",
    "nodemailer": "^6.9.9",
    "socket.io": "^4.7.4",
    "youtube-recommendation-crawler": "*"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.17",
    "@types/node-emoji": "^2.1.0",
    "@types/nodemailer": "^6.4.14",
    "eslint": "^8.56.0",
    "eslint-config-custom": "*",
    "swagger-autogen": "^2.23.7",
    "tsconfig": "*",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
}
